"postnr": "9912", "poststed": "Kirkenes", "total_driftsinntekt": 2.45, "companies": [{"name": "BLOMSTERSPIREN AS", "driftsinntekt": 2.45}]}];

    const map = L.map('map').setView([64.5, 11], 4.7);

    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
      maxZoom: 18
    }).addTo(map);

    async function loadGeo() {
      const r = await fetch('https://raw.githubusercontent.com/carestad/norwegian-geodata/main/postal-codes.json.gz');
      const buf = await r.arrayBuffer();
      return JSON.parse(pako.ungzip(new Uint8Array(buf), {to: 'string'}));
    }

    function formatMNOK(v) {
      if (v === null || v === undefined || isNaN(v)) return "NA";
      return v.toLocaleString('nb-NO', {
        minimumFractionDigits: 0,
        maximumFractionDigits: 2
      }) + " mNOK";
    }

    (async () => {
      const geo = await loadGeo();
      const heat = [];
      const markers = [];
      let maxVal = 0;

      companyData.forEach(loc => {
        const code = loc.postnr;
        const g = geo[code];
        if (!g || !g.senterpunkt) return;

        const [lon, lat] = g.senterpunkt.coordinates;
        const total = loc.total_driftsinntekt; // already mNOK
        if (total > maxVal) maxVal = total;

        heat.push([lat, lon, total]);

        let html = `<strong>${code} ${loc.poststed}</strong><br>
                    Total driftsinntekt: <strong>${formatMNOK(total)}</strong><br><br>
                    <em>Selskaper:</em><br><ul>`;
        loc.companies.forEach(c => {
          html += `<li>${c.name}: ${formatMNOK(c.driftsinntekt)}</li>`;
        });
        html += "</ul>";

        markers.push(
          L.circleMarker([lat, lon], {
            radius: 6,
            fillOpacity: 0.85
          }).bindPopup(html)
        );
      });

      const scaled = heat.map(([lat, lon, v]) => [lat, lon, v / maxVal]);

      L.heatLayer(scaled, {
        radius: 25,
        blur: 15
      }).addTo(map);

      markers.forEach(m => m.addTo(map));
    })();
  </script>
</body>
</html>
